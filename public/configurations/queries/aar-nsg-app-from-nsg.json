{
    "id":"aar-nsg-app-from-nsg",
    "title":"TBD",
    "service":"elasticsearch",
    "query":{
        "index":"{{index:nuage_dpi_flowstats}}",
        "type":"{{type:nuage_doc_type}}",
        "body":{
            "size":0,
            "query":{
                "bool":{
                    "must":[
                        {
                            "range":{
                                "timestamp":{
                                    "gte":"{{startTime:now-24h}}",
                                    "lte":"{{endTimeTime:now}}",
                                    "format":"epoch_millis"
                                }
                            }
                        }
                    ]
                }
            },
            "aggs": {
                "11": {
                    "filters": {
                        "filters": {
                            "SourceNSG": {
                                "query": {
                                    "term": {
                                        "SourceNSG": "{{snsg:default}}"
                                    }
                                }
                            }
                        }
                    },
                    "aggs": {
                        "SumofMB": {
                            "sum": {
                                "field": "TotalMB"
                            }
                        },
                        "SumofPackets": {
                            "sum": {
                                "field": "TotalPacketsCount"
                            }
                        },
                        "APMGroup": {
                            "terms": {
                                "field": "APMGroup",
                                "size": 5,
                                "order": {
                                    "SumofMB": "desc"
                                }
                            },
                            "aggs": {
                                "SumofMB": {
                                    "sum": {
                                        "field": "TotalMB"
                                    }
                                },
                                "SumofPackets": {
                                    "sum": {
                                        "field": "TotalPacketsCount"
                                    }
                                },
                                "Application": {
                                    "terms": {
                                        "field": "Application",
                                        "size": 5,
                                        "order": {
                                            "SumofMB": "desc"
                                        }
                                    },
                                    "aggs": {
                                        "SumofMB": {
                                            "sum": {
                                                "field": "TotalMB"
                                            }
                                        },
                                        "SumofPackets": {
                                            "sum": {
                                                "field": "TotalPacketsCount"
                                            }
                                        },
                                        "L7Classification": {
                                            "terms": {
                                                "field": "L7Classification",
                                                "size": 5,
                                                "order": {
                                                    "SumofMB": "desc"
                                                }
                                            },
                                            "aggs": {
                                                "SumofMB": {
                                                    "sum": {
                                                        "field": "TotalMB"
                                                    }
                                                },
                                                "SumofPackets": {
                                                    "sum": {
                                                        "field": "TotalPacketsCount"
                                                    }
                                                },
                                                "SrcVportName": {
                                                    "terms": {
                                                        "field": "SrcVportName",
                                                        "size": 5,
                                                        "order": {
                                                            "SumofMB": "desc"
                                                        }
                                                    },
                                                    "aggs": {
                                                        "SumofMB": {
                                                            "sum": {
                                                                "field": "TotalMB"
                                                            }
                                                        },
                                                        "SumofPackets": {
                                                            "sum": {
                                                                "field": "TotalPacketsCount"
                                                            }
                                                        },
                                                        "SrcUplink": {
                                                            "terms": {
                                                                "field": "SrcUplink",
                                                                "size": 5,
                                                                "order": {
                                                                    "SumofMB": "desc"
                                                                }
                                                            },
                                                            "aggs": {
                                                                "SumofMB": {
                                                                    "sum": {
                                                                        "field": "TotalMB"
                                                                    }
                                                                },
                                                                "SumofPackets": {
                                                                    "sum": {
                                                                        "field": "TotalPacketsCount"
                                                                    }
                                                                },
                                                                "SrcUplinkRole": {
                                                                    "terms": {
                                                                        "field": "SrcUplinkRole",
                                                                        "size": 5,
                                                                        "order": {
                                                                            "SumofMB": "desc"
                                                                        }
                                                                    },
                                                                    "aggs": {
                                                                        "SumofMB": {
                                                                            "sum": {
                                                                                "field": "TotalMB"
                                                                            }
                                                                        },
                                                                        "SumofPackets": {
                                                                            "sum": {
                                                                                "field": "TotalPacketsCount"
                                                                            }
                                                                        },
                                                                        "DestinationNSG": {
                                                                            "terms": {
                                                                                "field": "DestinationNSG",
                                                                                "size": 5,
                                                                                "order": {
                                                                                    "SumofMB": "desc"
                                                                                }
                                                                            },
                                                                            "aggs": {
                                                                                "SumofMB": {
                                                                                    "sum": {
                                                                                        "field": "TotalMB"
                                                                                    }
                                                                                },
                                                                                "SumofPackets": {
                                                                                    "sum": {
                                                                                        "field": "TotalPacketsCount"
                                                                                    }
                                                                                }
                                                                            }
                                                                        }
                                                                    }
                                                                }
                                                            }
                                                        }
                                                    }
                                                }
                                            }
                                        }
                                    }
                                }
                            }
                        }
                    }
                }
            }
        }
    }
}