{
    "id":"app-specific-vertical-bar",
    "title":"App Specific Vertical Bar",
    "service":"elasticsearch",
    "query":{
        "index":"{{index:nuage_dpi_flowstats}}",
        "type":"{{type:nuage_doc_type}}",
        "body":{
            "size":0,
            "query":{
                "bool":{
                    "must":[
                        {
                            "range":{
                                "timestamp":{
                                    "gte":"{{startTime:now-24h}}",
                                    "lte":"{{endTime:now}}",
                                    "format":"epoch_millis"
                                }
                            }
                        }
                    ]
                }
            },
            "aggs":{
                "4":{
                    "filters":{
                        "filters":{
                            "L7Classification":{
                                "query":{
                                    "term":{
                                        "L7Classification":"{{app:myApp-0}}"
                                    }
                                }
                            }
                        }
                    },
                    "aggs":{
                        "date-histo":{
                            "date_histogram":{
                                "field":"timestamp",
                                "interval":"30m"
                            },
                            "aggs":{
                                "SumOfBytes":{
                                    "sum":{
                                        "field":"TotalBytesCount"
                                    }
                                },
                                "SumOfPackets":{
                                    "sum":{
                                        "field":"TotalPacketsCount"
                                    }
                                }
                            }
                        }
                    }
                }
            }
        }
    }
}
